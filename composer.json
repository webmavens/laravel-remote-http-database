{
    "name": "webmavens/laravel-remote-http-database",
    "description": "Laravel database adapter for remote MySQL via HTTP API with encryption",
    "keywords": ["laravel", "database", "mysql", "remote", "http", "encryption"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\RemoteHttpDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\RemoteHttpDatabase\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\RemoteHttpDatabase\\RemoteHttpDatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
